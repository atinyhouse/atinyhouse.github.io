source "https://rubygems.org"

# GitHub Pages compatible Jekyll setup
gem "github-pages", group: :jekyll_plugins

# Jekyll plugins
group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.12"
  gem "jekyll-paginate"
  gem "jekyll-seo-tag"
end

# Markdown parser
gem "kramdown-parser-gfm"

# Web server for local development
gem "webrick", "~> 1.7"